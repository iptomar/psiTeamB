//As roles e permissões do Gestor de Sistema estão feitas!!!!!!!!!!!!
//*************************Gestor do Sistema***********************//

function (user, context, callback) {
  var namespace = 'https://app.pt/';
  context.accessToken[namespace + 'user_authorization'] = {
    "groups": ["Todos os Users"]
    "roles": ["Gestor"],
    "permissions": ["Vê	o seu perfil "]
  };
  return callback(null, user, context);
}


function (user, context, callback) {
  var namespace = 'https://tojs.io/';
  context.accessToken[namespace + 'user_authorization'] = {
    "groups": ["Gestor do Sistema"]
    "roles": ["Gestor"],
    "permissions": ["Cria uma conta do coordenador de Equipa"]
  };
  return callback(null, user, context);
}

